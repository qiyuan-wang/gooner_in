#main
  .container
    .row
      #mainbar.span9.box
        = form_tag sessions_path, method: :post do
          .field
            = label_tag :email, 'Email:'
            = text_field_tag :email
          .field
            = label_tag :password, '密码:' 
            = password_field_tag :password
          .actions
            = submit_tag '登录'
